{
  "youtube": {
    "title": "$NAME on $MAP - $TIME",
    "description": [
      "Player: $NAME",
      "Map: $MAP",
      "Class: $CLASS",
      "Time: $TIME $SPLIT",
      "Date: $DATE",
      "",
      "Map download: $MAP_URL",
      "Demo download: $DEMO_URL",
      "",
      "This video was automatically generated on $DATETIME",
      "https://github.com/laurirasanen/tempusrecords",
      "",
      "Recorded with Source Video Render by crashfort",
      "https://github.com/crashfort/SourceDemoRender"
    ],
    "collectionTitle": "$ZONETYPE collection #$NUMBER",
    "courseCollectionTitle": "$MAP course collection",
    "collectionDescription": [
      "This video was automatically generated on $DATETIME",
      "https://github.com/laurirasanen/tempusrecords",
      "",
      "Recorded with Source Video Render by crashfort",
      "https://github.com/crashfort/SourceDemoRender"
    ],
    "video_limit": 65,
    "publishDelay": 30
  },
  "video": {
    "deleteCompressed": false,
    "deleteUncompressed": false,
    "maxRunsInCollection": 35,
    "mapMaxDuration": 90,
    "extraMaxDuration": 1,
    "extraMinAge": 0,
    "mapMinAge": 7,
    "startPadding": 3,
    "endPadding": 3,
    "ffmpegOptions": [
      "-movflags faststart",
      "-c:v libx264",
      "-crf 20",
      "-profile:v high",
      "-level:v 4.2",
      "-preset:v veryfast",
      "-tune:v film",
      "-bf 2",
      "-g 30",
      "-coder 1",
      "-pix_fmt yuv420p",
      "-map 0:v:0",
      "-map 1:a:0",
      "-c:a aac",
      "-profile:a aac_low",
      "-b:a 384k"
    ],
    "text": {
      "ffmpegOptions": {
        "font": "Segoe UI Light",
        "fontsize": "h/15",
        "fontcolor": "#FFFFFF"
      },
      "position": {
        "bottomLeft": {
          "x": "5",
          "y": "h - th - 5"
        },
        "topLeft": {
          "x": "5",
          "y": "h - h/7.5"
        },
        "bottomRight": {
          "x": "w - tw - 5",
          "y": "h - th - 5"
        },
        "topRight": {
          "x": "w - tw - 5",
          "y": "h - h/7.5"
        }
      },
      "displayDuration": 3,
      "fadeInDuration": 0.5,
      "fadeOutDuration": 0.5,
      "endPadding": 0,
      "startPadding": 0,
      "maxAlpha": 0.8
    },
    "fadeInDuration": 0.25,
    "fadeOutDuration": 0.5,
    "allowMissingCourses": true
  },
  "audio": {
    "fadeInDuration": 1,
    "fadeOutDuration": 1.5
  },
  "rcon": {
    "address": "127.0.0.1",
    "port": 27015,
    "password": "bananaman",
    "listenAddress": "127.0.0.1",
    "listenPort": 3002
  },
  "tf2": {
    "path": "C:/Program Files (x86)/Steam/steamapps/common/Team Fortress 2/tf/",
    "launch": "C:/Program Files (x86)/Steam/steamapps/common/Team Fortress 2/hl2.exe",
    "args": "-game tf -dxlevel 98 -novid -windowed -w 1280 -h 720 +exec cinema +volume 0 +stopsound +rcon_address 127.0.0.1:3002 +rcon_password bananaman -nomouse"
  },
  "svr": {
    "path": "../svr",
    "launch": "../svr/svr_game_launcher_cli.exe",
    "args": "tf2-win",
    "recordingFolder": "../svr/data/movies"
  }
}
